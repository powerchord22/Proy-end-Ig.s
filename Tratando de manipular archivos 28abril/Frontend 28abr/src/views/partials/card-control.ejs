
<section class="container-fluid" id="servicios">
  <h2>Servicios por Especialista</h2><hr class="subrayado">
  <div class="row alert alert-light">
      <!--Grid de Boostrap5.2 de las columnas iconos del avatar  https://www.iconfinder.com/-->
      <% profesional.forEach (function(control){ %>
      <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12 col-12">
          <div class="card" style="width: 18rem;">
              <div class="card-body d-grid gap-2 mx-auto col-xl-10 col-lg-10 col-md-8 col-sm-7 col-10">
                
                  <h5 class="card-title"><%=control.nombre%></h5>
                  <h5 class="card-title"><%=control.especialidad%></h5>
                  <a href="#" class="btn btn-outline-success">Buscar <i class="bi bi-search"></i></a>
              </div>
          </div>
      </div>
      <% }); %>
  </div>
</section>

