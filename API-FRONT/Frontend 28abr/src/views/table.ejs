<table class="table table-bordered table-striped table-hover text-center mt-4 border border-light table"
id="example">
<thead>
    <tr class="bg-success text-white border">
        <th>id</th>
        <th>Nombre</th>
        <th>
            <div class="col-sm-12"><input class="form-control" type="text" id="inputRut"
                    placeholder="Buscar por RUT..."></div>
        </th>
        <th>Asesoria</th>
        <th>Fecha</th>
        <th>Configuraci√≥n</th>
    </tr>
</thead>
<tbody>
    <% profesional.forEach((pac)=>{ %>
        <tr>
            <td>
                <%= pac.id %>
            </td>
            <td>
                <%= pac.nombre %>
            </td>
            <td>
                <%= pac.rut %>
